{
  "env": {
    "browser": true,
    "es2021": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "bootstrap": "readonly",
    "mapboxgl": "readonly",
    "CountUp": "readonly",
    "GlobalJobTracker": "readonly",
    "showNotification": "readonly",
    "axios": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "error"
  }
}
